<% include head %>
<body style="background-color:darkslategrey">
<script type="text/javascript">
	function add_feed()
	{
		var div1 = document.createElement('div');
		// Get template data
		div1.innerHTML = document.getElementById('newlinktpl').innerHTML;
		// append to our form, so that template data
		//become part of form
		document.getElementById('newlink').appendChild(div1);
	}
	</script>
<div class="super_container">
	
	<% include pitchupheader%>			
	<% include menu%>
    
		<div class="container" style="margin-top:20px;background-color:floralwhite">
			<br><br><br><br>
			<center>
				
			<h1><font color='black'>Build your profile</font></h1>
			</center>	
			<br>
			<font color='black'>
			<form method='POST' action='/pitchup/profile' enctype="multipart/form-data">
				<div class="container" style="max-width:60%">
				<div class="form-group">
					<label for="exampleInputEmail1">Name</label>
					<input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter Name of startup", name='nm'>
				  </div>
				  <div class="form-group">
					<label for="exampleInputPassword1">Location</label>
					<input type="text" class="form-control" id="exampleInputPassword1" placeholder="Enter the location for your startup" name='location'>
				  </div>
				  <div class="form-group">
					<label for="exampleInputPassword1">Company Size</label>
					<input type="text" class="form-control" id="exampleInputPassword1" placeholder="Enter company size" name='c_size'>
				  </div>
				  <div class="form-group">
					<label for="exampleInputPassword1">Market Category</label>
					<input type="text" class="form-control" id="exampleInputPassword1" placeholder="E-Commerce/Educational/Social-Networking etc." name='m_cat'>
				  </div>
				  <div class="form-group">
						<label for="exampleInputPassword1">Company Bio</label>
						<textarea name='bio' class="form-control" placeholder="Write a short bio, include history,founder,growth etc."></textarea>
					  </div>  
				  <div class="form-group">
						<label for="exampleInputPassword1">CEO</label>
						<input type="text" class="form-control" id="exampleInputPassword1" placeholder="Enter the name of CEO" name='ceo'>
				  </div>
				  <div class="form-group">
						<label for="exampleInputPassword1">GSTIN Number</label>
						<input type="text" class="form-control" id="exampleInputPassword1" placeholder="Enter the registered GSTIN number of your startup" name='gstin'>
				  </div>
				  <div class="form-group">
						<label for="exampleInputPassword1">Display Picture</label>
						<input type="file" class="form-control" name="img">
					 </div>
				<button type="submit" class="btn btn-primary">Submit</button>
			</div>
			</form>
		</font>
		</div>
	</div>

	<!-- Converter -->

	<div class="converter">
		
		<div class="container">
			
		</div>
	</div>

	<!-- Info -->

	<div class="info">
		<div class="container">
		THIS IS Pitchup PAGE	
		</div>
	</div>

	<!-- News -->

	<div class="news">
		<div class="container">
			
		</div>
	</div>

	<% include footer%>

<script src="js/jquery-3.2.1.min.js"></script>
<script src="styles/bootstrap4/popper.js"></script>
<script src="styles/bootstrap4/bootstrap.min.js"></script>
<script src="plugins/OwlCarousel2-2.2.1/owl.carousel.js"></script>
<script src="plugins/easing/easing.js"></script>
<script src="plugins/parallax-js-master/parallax.min.js"></script>
<script src="js/custom.js"></script>
</body>
</html>